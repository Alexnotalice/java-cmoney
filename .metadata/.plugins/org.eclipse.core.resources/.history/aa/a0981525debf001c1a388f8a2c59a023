import java.util.Scanner;

public class St104{
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int num=sc.nextInt();
    int numlth=String.valueOf(num).length();
    int []numbers = new int[11];
    int temp=0;    
    temp=num; 
    for(int i=0;i<numlth;i++){           
        numbers[i]=temp%10;
        temp=temp/10;
    }       
    String str="";
    for(int s=0;s<numlth;s++){
        str = str + Integer.toString(numbers[s]);
    } 
    System.out.printf("%d-",0321);
    // System.out.print(1);
    // System.out.print(2
    // );
  }
}
